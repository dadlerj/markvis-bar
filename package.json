{
  "name": "markvis-bar",
  "version": "0.0.5",
  "description": "Bar chart generator for markvis in browser and node.js",
  "main": "./dist/bundle.js",
  "module": "index",
  "scripts": {
    "example": "node ./example",
    "lint": "xo",
    "build": "rm -rf ./dist && mkdir dist && ./node_modules/.bin/babel src --presets=es2015 | ./node_modules/.bin/uglifyjs --compress --mangle --output dist/bundle.js",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/geekplux/markvis-bar.git"
  },
  "keywords": [
    "d3",
    "vis",
    "visualization",
    "markvis",
    "markvis-plugin",
    "markvis-bar"
  ],
  "author": "GeekPlux <geekplux@gmail.com> (https://github.com/geekplux)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "d3-node": "^1.0.3",
    "d3node-output": "^0.0.9",
    "eslint-config-geekplux": "^1.0.3",
    "uglify-js": "^3.0.24",
    "xo": "^0.18.2"
  },
  "xo": {
    "extends": "geekplux",
    "esnext": true,
    "envs": [
      "jest"
    ]
  }
}
